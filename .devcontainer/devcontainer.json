{
    "name": "DPS",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "service": "vscode",
    "runServices": [
        "vscode"
    ],
    "shutdownAction": "stopCompose",
    "workspaceFolder": "/workspace",
    "postCreateCommand": "",
    "features": {
        "ghcr.io/devcontainers/features/sshd": {},
        "ghcr.io/tailscale/codespace/tailscale": {}
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-azuretools.vscode-docker",
                "donjayamanne.python-extension-pack"
            ],
            "settings": {
                // General settings
                "files.eol": "\n"
            }
        }
    }
}